{
  "name": "lieynime",
  "version": 2,
  "framework": "php",
  "buildCommand": "",
  "devCommand": "php -S localhost:3000",
  "installCommand": "",
  "env": {
    "PHP_VERSION": "8.2"
  },
  "routes": [
    {
      "src": "/(.*\\.php)$",
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.js)$",
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.css)$",
      "dest": "/$1"
    },
    {
      "src": "/(.+)",
      "dest": "/index.php"
    },
    {
      "src": "/",
      "dest": "/index.php"
    }
  ]
}
